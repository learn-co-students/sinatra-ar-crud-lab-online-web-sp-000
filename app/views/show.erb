<h1>Article</h1>
 
<div class="article">
  <h3>Title: <%= @article.title %></h3><br>
  <h4>Content: <%= @article.content %></h4>
</div><br>
 
<form method="post" action="/articles/<%= @article.id %>">
  <input id="hidden" type="hidden" name="_method" value="DELETE">
  <input type="submit" value="delete">
</form>
