<!doctype html>

<html lang="en">
<head>
  <meta charset="utf-8">
  <title>Articles</title>
</head>

<body>
  <h1>Articles</h1>

  <ul>
  <% @articles.each do |article|%>
    <li>
      <a href="/articles/<%= article.id %>"><%= article.title %></a>
      <p><%=article.content %></p>
    </li>
  <% end %>
  </ul>
</body>
</html>
