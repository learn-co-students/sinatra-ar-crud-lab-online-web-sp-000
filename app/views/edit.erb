<form action="/articles/<%= @article.id %>" method="POST">
<input id="hidden" type="hidden" name="_method" value="PATCH">
   <label for="title">Article Title:</label><br>
   <input id="title" type="text" name="title" value="<%= @article.title %>"/><br>

   <label for="content">Article Content:</label><br>
   <input id="content" type="text" name="content" value="<%= @article.content %>"/><br>

    <input type="submit" value="submit" name="submit"/>
   
</form>
