    <form action="/articles/<%= @article.id %>" method="post">
      <input id="hidden" type="hidden" name="_method" value="PATCH"><br>
      <h1><%= @article.title %></h1>
      <p><%=@article.content %></p>
      <p>Name: <input id="name" type="text" name="title"></p>
      <p>Content: <input id="content" type="text" name="content"></p>
      <input type="submit" value="submit">
    </form>

  
